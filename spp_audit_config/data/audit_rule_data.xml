<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Program Rule -->
    <function model="spp.audit.rule" name="create_rules">
        <value name="rule_name">Program Rule</value>
        <value name="model">g2p.program</value>
        <value name="fields_to_log" eval="['name']" />
    </function>

