<odoo>
    <record id="create_event_data_form_view" model="ir.ui.view">
        <field name="name">create_event_data_form_view</field>
        <field name="model">spp.create.event.wizard</field>
        <field name="arch" type="xml">
            <form edit="0">
                <sheet>
                    <group>
                      <field name="partner_id" readonly="1" />
                      <field name="event_data_model" />
                      <field name="registrar" />
                      <field name="collection_date" />
                      <field name="expiry_date" />
                    </group>
                </sheet>
                <footer>
                    <button name="next_page" string="Next" type="object" class="oe_highlight" />
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
