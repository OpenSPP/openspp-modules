<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_groups_event_house_visit_form" model="ir.ui.view">
        <field name="name">view_groups_event_house_visit_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="spp_event_data.view_groups_event_data_form" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='event_data_group']" position="inside">
              <field name="active_house_visit" />
            </xpath>
            <xpath expr="//group[@name='event_data_group2']" position="inside">
              <field name="active_phone_survey" />
            </xpath>
        </field>
    </record>
</odoo>
