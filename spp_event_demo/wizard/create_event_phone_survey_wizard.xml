<odoo>
    <record id="create_event_phone_survey_form_view" model="ir.ui.view">
        <field name="name">create_event_phone_survey_form_view</field>
        <field name="model">spp.create.event.phone.survey.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                      <field name="summary" />
                      <field name="description" />
                    </group>
                </sheet>
                <footer>
                    <button name="create_event" string="Save" type="object" class="oe_highlight" />
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
