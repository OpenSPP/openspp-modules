<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="inherit_view_program_list_form" model="ir.ui.view">
        <field name="name">inherit_view_program_list_form</field>
        <field name="model">g2p.program</field>
        <field name="priority">1</field>
        <field name="inherit_id" ref="g2p_programs.view_program_list_form" />
        <field name="arch" type="xml">
            <xpath expr="//button[@name='deduplicate_beneficiaries']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//button[@name='notify_eligible_beneficiaries']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
        </field>
    </record>

</odoo>
