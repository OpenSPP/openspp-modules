<odoo>
    <record id="view_spp_event_poverty_indicator_tree" model="ir.ui.view">
        <field name="name">view_spp_event_poverty_indicator_tree</field>
        <field name="model">spp.event.poverty.indicator</field>
        <field name="priority">1</field>
        <field name="arch" type="xml">
            <tree>
                <field name="type_of_housing" />
            </tree>
        </field>
    </record>

