            <field name="fa_icon">fa fa-users</field>
            <field name="fa_color">#6495ED;</field>
            <field name="tile_color">#6495ED;</field>
            <field name="text_color">#FFFFFF;</field>
            <field name="filter">[('program_id','=','active_id')]</field>
